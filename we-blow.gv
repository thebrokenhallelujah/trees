okwIE -> augaON
{ vindrON augaON } -> vindaugaON
vindaugaON -> windoweME [style=dotted]
windoweME -> window

/* ventilate */
ventusL [label=<<b>Latin</b><br/><font color="red">ventus</font><br/><font point-size="12">wind</font><br/><font color="red">ventulus</font>>]
