ventilatenME [label=<<b>Middle<br/>English</b><br/><font color="red">ventilaten</font><br/><font point-size="12">to blow away</font>>]
ventilate [label=<<b>Modern<br/>English</b><br/><font color="red">ventilate</font>>]
