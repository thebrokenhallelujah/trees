// hendecasyllabic
dekmIE [label=<<b>Indo-<br/>European</b><br/><font color="red">*dekm̥</font><br/><font point-size="12">ten</font>> shape=box style="dashed,rounded" URL="dekm-ten1.svg"]
dekaGk [label=<<b>Greek</b><br/><font color="red">deka</font>>]
