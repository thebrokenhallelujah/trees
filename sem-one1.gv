upoIE [label=<<b>Indo-<br/>European</b><br/><font color="red">*upo</font><br/><font point-size="12">under</font>> shape=box style="dashed,rounded" URL="upo-under.svg"]
hupoGk [label=<<b>Greek</b><br/><font color="red">hupo</font>>]
